        "sellers": "http://127.0.0.1:8001/sellers",
        "customers": "http://127.0.0.1:8001/customers",
        "shipments": "http://127.0.0.1:8001/shipments"
    },
    "customerConfig": {
        "maxNumberKeysToBrowse": 10,
