{
    "transactionDistribution": {
        "CUSTOMER_SESSION": 100// 70,
        // "UPDATE_DELIVERY": 100
    },
    // not inside customer config because customer
    // worker does not need to know that
    "customerDistribution": "NON_UNIFORM",
    "executionTime": 600000, // 1 minute by default
    "concurrencyLevel" : 1, // 10000,
    // mandatory entry points
  "mapTableToUrl": {
    "products": "http://127.0.0.1:5008",
    "carts": "http://127.0.0.1:5001",
    "sellers": "http://127.0.0.1:5006",
    "customers": "http://127.0.0.1:5007",
    "shipments": "http://127.0.0.1:5005"
  },
  "streamingConfig": {
    "type":  "REDIS",
    "host": "localhost",
    "port": 6379,
    "streams": [
      "InvoiceIssued",
      "PaymentConfirmed",
      "PaymentFailed",
      "ShipmentNotification",
      "Product",
      "ReserveStock",
      "DeliveryNotification",
      "StockItem",
      "StockConfirmed",
      "ReserveStockFailed"
    ],
    "txStreams": [ "TransactionMark" ]
  },
  "customerWorkerConfig": {
    "maxNumberKeysToBrowse": 10,
    "maxNumberKeysToAddToCart": 10,
    "sellerDistribution": "NON_UNIFORM",
    "minMaxQtyRange": {
      "min": 1,
      "max": 10
    },
    "delayBetweenRequestsRange": {
      "min": 1,
      "max": 1000
    },
    "checkoutProbability": 100,
    "voucherProbability": 5,
    "maxNumberVouchers": 10,
    "browsing": true
  },
  "sellerWorkerConfig": {
      "keyDistribution": "NON_UNIFORM",
      "delayBetweenRequestsRange": {
          "min": 1,
          "max": 1000
      }
  }
}

